{
  "name": "auto-kinto-alwaysdata",
  "version": "1.0.0",
  "description": "Elm client to deploy kinto to AlwaysData automatically",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/elm-make Main.elm --output=elm.js",
    "start": "node_modules/.bin/elm-reactor Main.elm",
    "live": "node_modules/.bin/elm-live Main.elm --output=elm.js -- --debug",
    "publish-to-gh-pages": "npm run build && cp index.html build/ && cp elm.js build/ && cp -r static/ build/ && node_modules/.bin/gh-pages --dist build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Natim/auto-kinto-alwaysdata.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Natim/auto-kinto-alwaysdata/issues"
  },
  "homepage": "https://github.com/Natim/auto-kinto-alwaysdata#readme",
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-live": "^2.6.0",
    "gh-pages": "^0.11.0"
  }
}
